{
  "name": "bouhnosaure/dogecoin-wallet-api",
  "type": "library",
  "description": "PHP library for interacting with the Dogecoin core wallet by json rpc",
  "keywords": [
    "bouhnosaure",
    "dogecoin-wallet-api"
  ],
  "homepage": "https://github.com/bouhnosaure/dogecoin-wallet-api",
  "license": "MIT",
  "authors": [
    {
      "name": "ci_trex",
      "email": "dev.citrex@gmail.com",
      "homepage": "https://github.com/bouhnosaure",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.4.3",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Bouhnosaure\\Dogecoin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bouhnosaure\\Dogecoin\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
